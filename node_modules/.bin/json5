esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../json5/lib/cli.js" "$@"
  ret=$?
else 
  node  "$basedir/../json5/lib/cli.js" "$@"
  ret=$?
fi
exit $ret
